	<question type="multichoice">
		<name>
			<text><![CDATA[Obtenir l'aide sur une commande]]></text>
		</name>
		<questiontext format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;">Dans un terminal Linux, quelle commande faut-il écrire pour obtenir la documentation sur la commande <span style="font-family: 'courier new', courier;">pwd </span>?</div><div style="font-family: Arial; font-size: 13px;">&nbsp;</div>]]></text>
		</questiontext>
		<externallink/>
		<usecase>1</usecase>
		<defaultgrade>1</defaultgrade>
		<editeur>0</editeur>
		<single>true</single>
		<answer fraction="0" format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;"><span style="font-family: 'courier new', courier;">cd pwd</span></div>]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
		<answer fraction="100" format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;"><span style="font-family: 'courier new', courier;">man pwd</span></div>]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
		<answer fraction="0" format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;"><span style="font-family: 'courier new', courier;">ls pwd</span></div>]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
		<answer fraction="0" format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;"><span style="font-family: 'courier new', courier;">mkdir pwd</span></div>]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
	</question>
	<question type="multichoice">
		<name>
			<text><![CDATA[Droits]]></text>
		</name>
		<questiontext format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;">On considère l'affichage suivant d'un terminal Linux :</div><div style="font-family: Arial; font-size: 13px; text-align: left;">&nbsp;</div><div style="font-family: Arial; font-size: 13px; text-align: left;"><span style="font-family: 'courier new', courier; background-color: #ced4d9; color: #34495e;">% ls -l</span><br><span style="font-family: 'courier new', courier; background-color: #ced4d9; color: #34495e;">total 0</span><br><span style="font-family: 'courier new', courier; background-color: #ced4d9; color: #34495e;">-rw-r----- 1 1003 nativel profs août &nbsp;25 09:47 Evaluation1</span></div><div style="font-family: Arial; font-size: 13px; text-align: left;">&nbsp;</div><div style="font-family: Arial; font-size: 13px; text-align: left;">Cocher l'affirmation exacte parmi les propositions ci-dessous :</div>]]></text>
		</questiontext>
		<externallink/>
		<usecase>1</usecase>
		<defaultgrade>1</defaultgrade>
		<editeur>0</editeur>
		<single>true</single>
		<answer fraction="0" format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;">Le fichier <span style="font-family: 'courier new', courier;">devoir<span style="font-family: arial, helvetica, sans-serif;"> n'est lisible que par son propriétaire.</span></span></div>]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
		<answer fraction="0" format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;">Personne ne peut modifier le fichier <span style="font-family: 'courier new', courier;">devoir.<span style="font-family: arial, helvetica, sans-serif;"><br></span></span></div>]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
		<answer fraction="0" format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;">Personne ne peut lire le fichier <span style="font-family: 'courier new', courier;">devoir<span style="font-family: arial, helvetica, sans-serif;">.</span></span></div><div style="font-family: Arial; font-size: 13px;">&nbsp;</div>]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
		<answer fraction="100" format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;">Un utilisateur appartenant au groupe <span style="font-family: 'courier new', courier;">profs<span style="font-family: arial, helvetica, sans-serif;"> peut lire le fichier devoir.</span></span></div>]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
