	<question type="multichoice">
		<name>
			<text><![CDATA[Libres et propriétaires]]></text>
		</name>
		<questiontext format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;">Quelle est la différence entre un logiciel libre et un logiciel propriétaire ?</div>]]></text>
		</questiontext>
		<externallink/>
		<usecase>1</usecase>
		<defaultgrade>1</defaultgrade>
		<editeur>0</editeur>
		<single>true</single>
		<answer fraction="100" format="plain_text">
			<text><![CDATA[On peut consulter le code source d'un logiciel libre, pas celui d'un logiciel propriétaire]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
		<answer fraction="0" format="plain_text">
			<text><![CDATA[Un logiciel libre est gratuit alors qu'un logiciel propriétaire est payant]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
		<answer fraction="0" format="plain_text">
			<text><![CDATA[Un logiciel propriétaire est plus sécurisé qu'un logiciel libre]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
		<answer fraction="0" format="plain_text">
			<text><![CDATA[Un logiciel propriétaire possède davantage de fonctionnalités qu'un logiciel libre]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
	</question>
	<question type="multichoice">
		<name>
			<text><![CDATA[Droits]]></text>
		</name>
		<questiontext format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;">Dans un terminal Linux, quelle commande permet d'attribuer à tous les utilisateurs le droit de lecture sur le fichier <span style="font-family: 'courier new', courier;">toto.txt </span>?</div>]]></text>
		</questiontext>
		<externallink/>
		<usecase>1</usecase>
		<defaultgrade>1</defaultgrade>
		<editeur>0</editeur>
		<single>true</single>
		<answer fraction="100" format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;"><span style="font-family: 'courier new', courier;">chmod a+r toto.txt</span></div>]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
		<answer fraction="0" format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;"><span style="font-family: 'courier new', courier;">chmod a+w toto.txt</span></div>]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
		<answer fraction="0" format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;"><span style="font-family: 'courier new', courier;">chmod a-r toto.txt</span></div>]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
		<answer fraction="0" format="html">
			<text><![CDATA[<div style="font-family: Arial; font-size: 13px;"><span style="font-family: 'courier new', courier;">chmod o+r toto.txt</span></div>]]></text>
			<feedback>
				<text><![CDATA[]]></text>
			</feedback>
		</answer>
	</question>
